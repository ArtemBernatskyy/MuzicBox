{
  "name": "muzicbox",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.1.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dotenv": "6.0.0",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.2.0",
    "lodash": "^4.17.10",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "raven-for-redux": "^1.3.1",
    "raven-js": "^3.26.4",
    "react": "16.5.2",
    "react-css-modules": "^4.7.1",
    "react-dom": "16.5.2",
    "react-dropzone": "^4.1.0",
    "react-error-overlay": "^4.0.0",
    "hammerjs": "2.0.8",
    "react-hammerjs": "github:raulrene/react-hammerjs",
    "react-notification-system": "^0.2.15",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-waypoint": "^8.0.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.3",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "watch": "NODE_PATH=src/ webpack --progress --colors --config config/webpack.config.dev.js",
    "build": "NODE_PATH=src/ node scripts/build.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "fsevents": "^1.2.4",
    "postcss": "^7.0.5",
    "react-dev-utils": "^3.1.1",
    "redux-devtools-extension": "^2.13.5",
    "style-loader": "0.21.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-manifest-plugin": "1.3.2"
  }
}
